{
  "manifest_version": 2,

  "name": "CampusSearch+",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "author": "Dr√© van Oorschot",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "icons": {
    "16":"icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://search.student.utwente.nl/*"],
      "js": ["main.js"]
    }
  ],
  "web_accessible_resources": [
    "clicker.js"
  ]
}
